{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "sourceMap": true,
    "target": "ES5",
    "module": "ESNext",
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "react",
    "allowJs": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
  },
  "include": ["./site/**/*", "./components/**/*"],
}
